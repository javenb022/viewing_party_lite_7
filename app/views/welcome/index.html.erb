<h1>Viewing Party</h1>

<% if session[:user_id] %>
  <%= link_to "Log Out", logout_path %>
<% else %>
  <%= link_to "Register a New User", new_user_path %>
  <%= link_to "Log In", login_path %>
<% end %>

<% if session[:user_id] %>
  <h2>Existing Users</h2>
  <% @users.each do |user| %>
  <ul>
    <li id="user_<%= user.id %>"><%= user.email %></li>
  </ul>
  <% end %>
<% end %>